{
  "expo": {
    "name": "weather-buddy-react-native",
    "slug": "weather-buddy",
    "version": "1.0.0",
    "orientation": "Weather Buddy app is basically the app version of the web version on my github",
    "icon": "./assets/Weather-Buddy-Logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/Weather-Buddy-Logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#111013"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "We need your location to provide weather updates."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "ACCESS_FINE_LOCATION"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "222678db-8acc-4f8e-82d8-8522ce514c44"
      }
    },
    "owner": "andrew_zerdelian"
  }
}
